<h1>Listing imagenes</h1>

<table>
  <tr>
    <th>Cuerpo</th>
    <th>Objeto</th>
    <th>Imagen url</th>
    <th>Fecha creacion</th>
    <th>Fecha actualizacion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @imagenes.each do |imagen| %>
  <tr>
    <td><%= imagen.cuerpo_id %></td>
    <td><%= imagen.objeto_id %></td>
    <td><%= imagen.imagen_url %></td>
    <td><%= imagen.fecha_creacion %></td>
    <td><%= imagen.fecha_actualizacion %></td>
    <td><%= link_to 'Show', imagen %></td>
    <td><%= link_to 'Edit', edit_imagen_path(imagen) %></td>
    <td><%= link_to 'Destroy', imagen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Imagen', new_imagen_path %>
